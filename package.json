{
  "name": "react-seed",
  "version": "0.0.1",
  "description": "a scaffold for building React apps with ES7 and webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "analyse": "webpack --config ./config/webpack.analyse.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "doc": "jsdoc src -r"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:marcosun/react-seed.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Marco",
  "license": "UNLICENSED",
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "globals": {
      "BABEL_ENV": "test",
      "NODE_ENV": "test"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "material-ui": "^1.0.0-beta.37",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.3",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-form-saga": "^0.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "eslint": "^4.9.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^6.1.0",
    "jest": "^21.2.1",
    "jsdoc": "^3.5.5",
    "react-dev-utils": "^11.0.4",
    "react-test-renderer": "^16.0.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^1.0.1",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "browserslist": [
    "defaults",
    "Chrome >= 40",
    "ie >= 9"
  ]
}
