{
  "name": "react-seed",
  "version": "0.0.1",
  "description": "a scaffold for building React apps with ES7 and webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "doc": "jsdoc src -r"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab05.dtdream.com:marco/react-seed.git"
  },
  "keywords": [
    "react",
    "seed",
    "pubtrans"
  ],
  "author": "Marco",
  "license": "UNLICENSED",
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "globals": {
      "BABEL_ENV": "test",
      "NODE_ENV": "test"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "material-design-icons": "^3.0.1",
    "material-ui": "^1.0.0-beta.19",
    "material-ui-icons": "^1.0.0-beta.17",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "typeface-roboto": "0.0.43"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "# 1.x.x version of file-loader is not compatible with a bunch of loaders, i.e. css-loader, html-loader, ..., https://github.com/webpack-contrib/file-loader/issues/181#issuecomment-318230691, https://github.com/webpack-contrib/html-loader/issues/140#issuecomment-320546696",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^5.6.0",
    "jest": "^21.2.1",
    "jsdoc": "^3.5.5",
    "postcss-loader": "^2.0.8",
    "react-dev-utils": "^4.1.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "browserslist": [
    "defaults",
    "Chrome >= 40",
    "ie >= 9"
  ]
}
